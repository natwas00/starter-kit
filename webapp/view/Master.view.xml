<mvc:View controllerName="stk.starterkit.controller.Master"
    xmlns:mvc="sap.ui.core.mvc" displayBlock="true"
    xmlns="sap.m">
    <Shell id="shell">
    <App id="app">
    <pages>
    <Page id="page" title="{i18n>title}">
        <content>
    

          <Table id="_IDGenTable1" items ="{/Customers}">
          <columns>
          <Column id="_IDGenColumn1" >
                <Label id="_IDGenLabel1" text="{i18n>companyName}"/>
          </Column>
         <Column id="_IDGenColumn2" >
                <Label id="_IDGenLabel2" text="{i18n>country}"/>
          </Column>
          </columns>
          <items>
          <ColumnListItem id="_IDGenColumnListItem1">
          <Text id="_IDGenText1" text="{CompanyName}"/>
          <Text id="_IDGenText2" text="{Country}"/>
          </ColumnListItem>
          </items>
          </Table>
         </content>
    </Page>
    </pages>
    </App>
    </Shell>
</mvc:View>
